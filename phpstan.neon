includes:
    - vendor/phpstan/phpstan-strict-rules/rules.neon

parameters:
    level: max
    paths:
        - library
        - bootstrap
        - src
        - tests
    tmpDir: var/phpstan
    ignoreErrors:
        - '#Method .+ has parameter \$container with generic class LeanPHP\\Container but does not specify its types: ServiceType#' # needed until we figure out the genericity in the Container